#include "vehicule2.h"

class Velo : public Vehicule
{
    public:
        Velo(int etat = 1, 
		bool antivol = false, 
		int couleur = 0, 
		int prix = 10000);
        ~Velo();

        void afficheType(); //affiche "Je suis un velo"

        bool aUnAntivol();
        void aUnAntivol(bool presenceAntivol);

    private:
        bool m_antivol;
};
