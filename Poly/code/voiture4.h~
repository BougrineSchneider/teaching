#include "vehicule3.h"

class Voiture : public Vehicule
{
    public:
        Voiture(int etat = 1, int fuel = 100, 
		int couleur = 0, int prix = 10000);
        ~Voiture();

        virtual void description(); 
        void setFuel(int fuel);
        int getFuel();

    private:
        int m_fuel;
};

